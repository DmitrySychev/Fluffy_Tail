<br><br>

<%= form_for @user do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <br>

    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <br>
    
    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <br>

    <%= f.submit "Sign Up"%><br>
    <% end %>

